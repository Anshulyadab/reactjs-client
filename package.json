{
  "name": "postgres-react-app",
  "version": "1.0.0",
  "description": "React app with PostgreSQL database connection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run check-ports && npm run build && npm run server",
    "dev:simple": "npm run build && npm run server",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "vercel-build": "npm run install-client && npm run build",
    "vercel-dev": "npm run dev",
    "get-credentials": "node scripts/get-vercel-credentials.js",
    "check-ports": "node scripts/check-ports.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest tests/auth.test.js",
    "test:database": "jest tests/database.test.js",
    "test:data": "jest tests/data.test.js",
    "deploy": "scripts/deploy.bat",
    "deploy:unix": "scripts/deploy.sh"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.11.3",
    "vercel": "^48.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "react",
    "postgresql",
    "express",
    "node"
  ],
  "author": "",
  "license": "MIT"
}
